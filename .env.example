APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
# Must match the URL you use to open the app (e.g. http://127.0.0.1:8000 with php artisan serve).
# In production (e.g. DigitalOcean), use https: https://your-app.ondigitalocean.app
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
# Default disk. In production with DO Spaces use: FILESYSTEM_DISK=spaces and FILESYSTEM_PRIVATE_DISK=spaces
FILESYSTEM_DISK=local
FILESYSTEM_PRIVATE_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# DigitalOcean Spaces (production). Set FILESYSTEM_DISK=spaces and FILESYSTEM_PRIVATE_DISK=spaces when APP_ENV=production
DO_SPACES_KEY=
DO_SPACES_SECRET=
DO_SPACES_BUCKET=storage-reyes-primero-app
DO_SPACES_ENDPOINT=https://sfo2.digitaloceanspaces.com
DO_SPACES_REGION=sfo2
# Optional: CDN URL if you use a custom domain for the Space
# DO_SPACES_URL=https://your-space.sfo2.digitaloceanspaces.com

VITE_APP_NAME="${APP_NAME}"
